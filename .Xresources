urxvt.termName: rxvt-unicode-256color
urxvt.scrollBar: false
urxvt.thickness: 0
urxvt.cursorBlink: 1
urxvt.urugentOnBell: false
!! Font preferenes

urxvt.font: xft:Source Code Pro for Powerline-10, xft:DejaVu Sans-9, xft:FontAwesome:size=5
urxvt.boldFont: xft:Source Code Pro for Powerline-10:weight=bold, xft:DejaVu Sans-9:wieght=bold, xft:FontAwesome:size=5
urxvt.letterSpace: 0
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.rgba: none
Xft.dpi: 96
!! Larger history limit
urxvt.saveLines: 65535

! Some keyboard setts

urxvt.perl-ext-common: default,clipboard
urxvt.iso14755: false 
urxvt.keysym.Control-Shift-C: perl:clipboard:copy
urxvt.keysym.Control-Shift-V: perl:clipboard:paste
urxvt.clipboard.copycmd: xsel -ib
urxvt.clipboard.pastecmd: xsel -ob

urxvt*scrollTtyOutput: false
urxvt*scrollTtyKeypress: true
urxvt*scrollWithBuffer: true

urxvt.keysym.Control-Left: \033[1;5D
urxvt.keysym.Control-Right: \033[1;5C

!! Borders

urxvt.externalBorder: 0
urxvt.internalBorder: 7
urxvt.borderLess: false


! Colors - Tomorrow Theme

